{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Serve",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/uvicorn",
            "cwd": "${workspaceFolder}",
            "args": [
                "unitree.app:app"
            ]
        },
        {
            "name": "populate",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/populate.py",
            "cwd": "${workspaceFolder}",
            "args": [
                "--max-depth=6",
                "--max-children=4",
                "--min-children=4"
            ]
        }
    ]
}
